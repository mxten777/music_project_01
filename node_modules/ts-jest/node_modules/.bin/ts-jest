#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/musicoding/music_project_01/node_modules/.pnpm/ts-jest@29.4.2_@babel+core@_e4f9fcc1972b3672a481d1dcc6830f16/node_modules/ts-jest/node_modules:/mnt/c/musicoding/music_project_01/node_modules/.pnpm/ts-jest@29.4.2_@babel+core@_e4f9fcc1972b3672a481d1dcc6830f16/node_modules:/mnt/c/musicoding/music_project_01/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/musicoding/music_project_01/node_modules/.pnpm/ts-jest@29.4.2_@babel+core@_e4f9fcc1972b3672a481d1dcc6830f16/node_modules/ts-jest/node_modules:/mnt/c/musicoding/music_project_01/node_modules/.pnpm/ts-jest@29.4.2_@babel+core@_e4f9fcc1972b3672a481d1dcc6830f16/node_modules:/mnt/c/musicoding/music_project_01/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../cli.js" "$@"
else
  exec node  "$basedir/../../cli.js" "$@"
fi
